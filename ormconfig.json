{
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "database": "awesome_nest",
    "synchronize": true,
    "logging": false,
    "entities": [
        "src/concepts/**/**.entity.ts"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "migrationsDir": "src/migration"
    }
}
